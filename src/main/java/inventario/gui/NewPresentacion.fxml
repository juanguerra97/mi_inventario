<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import org.controlsfx.control.textfield.CustomTextField?>

<HBox alignment="CENTER" prefHeight="85.0" prefWidth="350.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inventario.gui.NewPresentacionController">
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
   <children>
      <Label text="%nombre" />
      <CustomTextField fx:id="fieldNombre" promptText="%ingresar.nombre" HBox.hgrow="ALWAYS" />
      <Button fx:id="btnGuardar" mnemonicParsing="false" onAction="#onGuardar" text="%guardar" />
   </children>
</HBox>
